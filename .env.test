# Symfony Environment Variables
APP_ENV=dev
APP_SECRET=your_unique_dev_secret_here_change_this
DATABASE_URL="postgresql://app:!ChangeMe!@127.0.0.1:5433/app?serverVersion=16&charset=utf8"

